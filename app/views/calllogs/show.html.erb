<div class="page-header">
  <%= link_to calllogs_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_calllog_path(@calllog), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show calllog</h1>
</div>

<dl class="dl-horizontal">
  <dt>User:</dt>
  <dd><%= @calllog.user_id %></dd>

  <dt>Caller:</dt>
  <dd><%= @calllog.caller %></dd>

  <dt>Duration:</dt>
  <dd><%= @calllog.duration %></dd>

  <dt>Called on:</dt>
  <dd><%= @calllog.called_on %></dd>

</dl>
