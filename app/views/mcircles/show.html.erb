<div class="page-header">
  <%= link_to mcircles_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_mcircle_path(@mcircle), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show mcircle</h1>
</div>

<dl class="dl-horizontal">
  <dt>Number:</dt>
  <dd><%= @mcircle.number %></dd>

  <dt>State:</dt>
  <dd><%= @mcircle.state %></dd>

  <dt>Network:</dt>
  <dd><%= @mcircle.network %></dd>

</dl>
